<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group5.controllers.LoginController">
    <children>
        <Button fx:id="login" layoutX="356.0" layoutY="404.0" mnemonicParsing="false" onAction="#requestLogin" text="Login" />
        <TextField fx:id="usernameInput" layoutX="198.0" layoutY="326.0" prefHeight="25.0" prefWidth="204.0" promptText="username" />
        <Button layoutX="198.0" layoutY="404.0" mnemonicParsing="false" text="Forgot Password" />
        <Label layoutX="269.0" layoutY="278.0" text="LOGIN">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <Button fx:id="mainmenu" layoutX="325.0" layoutY="566.0" mnemonicParsing="false" onAction="#backToMenu" text="Main Menu" />
      <PasswordField fx:id="passwordInput" layoutX="198.0" layoutY="364.0" prefHeight="25.0" prefWidth="204.0" promptText="password" />
      <Label fx:id="usernameRequired" layoutX="198.0" layoutY="348.0" text="*username required" textFill="RED" visible="false">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Label fx:id="passwordRequired" layoutX="198.0" layoutY="386.0" text="*password required" textFill="RED" visible="false">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
